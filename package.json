{
  "name": "cloudtoys",
  "private": true,
  "version": "1.1.1",
  "author": {
    "email": "soham@distinction.dev",
    "name": "Soham Dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/distinction-dev/cloudtoys.git"
  },
  "keywords": [
    "Electron",
    "Typescript",
    "Vite",
    "React"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf packages/main/dist && rimraf packages/preload/dist && rimraf packages/renderer/dist",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "npm run clean && cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "start": "rimraf logs && node scripts/watch.mjs",
    "lint": "eslint . --ext js,ts,jsx,tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext js,ts,jsx,tsx --fix --ignore-path .gitignore",
    "lint:main": "eslint packages/main --ext js,ts,jsx,tsx",
    "lint:preload": "eslint packages/preload --ext js,ts,jsx,tsx",
    "lint:renderer": "eslint packages/renderer --ext js,ts,jsx,tsx",
    "tsc": "npm run tsc:main && npm run tsc:preload && npm run tsc:renderer",
    "tsc:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "tsc:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "tsc:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "validate": "npm run lint && npm run tsc && npm run test",
    "lint-staged": "lint-staged",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "commit": "cz",
    "release": "electron-builder --config .electron-builder.config.js --publish always",
    "release:dry": "semantic-release --dry-run",
    "updateVersion": "semantic-release"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.409.0",
    "@aws-sdk/client-s3": "^3.583.0",
    "@aws-sdk/credential-providers": "^3.409.0",
    "@smithy/shared-ini-file-loader": "^2.2.0",
    "assert": "^2.1.0",
    "date-fns": "^2.30.0",
    "electron-log": "^5.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.4",
    "github-markdown-css": "^5.4.0",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-resizable-panels": "^2.0.16",
    "react-router-dom": "^6.15.0",
    "unified": "^11.0.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/electron-devtools-installer": "^2.2.4",
    "@types/lodash": "^4.14.200",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^24.4.0",
    "electron-builder": "^24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-devtools-vendor": "^1.2.0",
    "electron-notarize": "^1.2.2",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "playwright": "^1.39.0",
    "postcss": "^8.4.28",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "semantic-release": "^22.0.5",
    "semantic-release-export-data": "^1.0.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.2",
    "unplugin-auto-expose": "^0.0.5",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^0.34.6"
  },
  "main": "packages/main/dist/index.cjs",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
